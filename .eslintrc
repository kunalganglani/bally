{
  "parser": "@babel/eslint-parser",
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "import/extensions": 0,
    "quote-props": 0,
    "no-console": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0
  }
}
